#accordion.panel-group
  .panel.panel-default(ng-repeat="type in documentTypes")
    .panel-heading( data-toggle='collapse', data-parent='#accordion', ng-href='{{"#"+type.Tag}}')
      h4.panel-title
        i.fa.fa-sort-desc.fa-centering
        a {{type.Tag}}
          
    .panel-collapse.collapse(id="{{type.Tag}}")
        .panel-body
          div(ng-repeat="document in type.Documents")
            span {{document.Name}}
